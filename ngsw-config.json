{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/logo.png",
          "/assets/logo_w.png",
          "/assets/fonts/NotoSans-Regular.ttf",
          "/assets/fonts/NotoColorEmoji-Regular.ttf"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "cacheConfig": {
        "maxAge": "10s",
        "maxSize": 20,
        "strategy": "freshness",
        "timeout": "420"
      },
      "name": "fedicache",
      "urls": [
        "https://cache.wafrn.net/**",
        "https://media.wafrn.net/**",
        "https://api.wafrn.net/api/**"
      ]
    }
  ]
}
