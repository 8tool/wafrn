{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/logo.png",
          "/assets/logo_w.png"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "fedicache",
      "updateMode": "lazy",
      "installMode": "lazy",
      "resources": {
        "files": ["**"],
        "urls": [
          "https://wafrnmedia.b-cdn.net/**",
          "https://wafrncache.b-cdn.net/**"
        ]

      }
    }
  ],
  "dataGroups": [
    {
    "name": "cdn",
    "urls": [
    "https://wafrnmedia.b-cdn.net/"
    ],
    "cacheConfig": {
      "maxSize": 512,
      "maxAge": "2h",
      "strategy": "freshness"
    }
  }
]
}
