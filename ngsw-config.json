{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "navigationUrls": [
    "/dashboard",
    "/dashboard/**",
    "/profile",
    "profile/**",
    "/blog/**",
    "/post/**",
    "/search",
    "/search/**"
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/logo.png",
          "/assets/logo_w.png",
          "/assets/fonts/NotoSans-Regular.ttf",
          "/assets/fonts/NotoColorEmoji-Regular.ttf"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": ["https://api.wafrn.net/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 250,
        "maxAge": "1m"
      }
    }
  ]
}
